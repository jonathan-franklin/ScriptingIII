global proc ColorControl(float $Red, float $Green, float $Blue){
    float $Color[] = {$Red,$Green,$Blue};
    setAttr "nurbsCircle1.overrideEnabled" 1;
    setAttr ("nurbsCircle.overrideColor") $Color;
    print "Colored";
    //print $Color;
    }
    

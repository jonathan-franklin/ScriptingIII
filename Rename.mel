proc Renamer(string $rename)
{
    string $sel[] = `ls -selection -long`;
    string $holding[];
    int $numItems = size($sel);

    tokenize($rename, "#", $holding);
    int $i = 1;
    string $newSel;

    for ($obj in $sel)
    {
        $newSel = $holding[0] + $i + $holding[1];
        rename($obj, $newSel);
        
        $i += 1;
    }
    print($sel);

}

Renamer("Left_#_Jnt");